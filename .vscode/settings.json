{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignoreWords": [
    "booter",
    "nawb",
    "channel",
    "const",
    "entry",
    "init",
    "miao",
    "msg",
    "mutex",
    "name",
    "nodejs",
    "once",
    "params",
    "pl",
    "plugin",
    "react",
    "receiver",
    "render",
    "set",
    "to",
    "tree",
    "ui"
  ],
  "files.associations": {
    ".js": "javascript",
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.gyp": "python",
    "mutex": "cpp"
  },
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "workspace.isHidden": true,
  "files.exclude": {
    "bazel-bin": true,
    "bazel-nawb/bazel-out": true,
    "bazel-out": true,
    "bazel-testlogs": true,
    "bazel-nawb": true,
    ".aswb": true
  }
}