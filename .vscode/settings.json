{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignoreWords": [
    "booter",
    "nawb",
    "channel",
    "const",
    "entry",
    "init",
    "miao",
    "msg",
    "mutex",
    "name",
    "nodejs",
    "once",
    "params",
    "pl",
    "plugin",
    "react",
    "receiver",
    "render",
    "set",
    "to",
    "tree",
    "ui"
  ],
  "files.associations": {
    ".js": "javascript",
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.gyp": "python",
    "mutex": "cpp"
  },
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "files.exclude": {
    ".buckconfig": false,
    ".editorconfig": false,
    ".eslintignore": false,
    ".eslintrc.js": false,
    ".gitattributes": false,
    ".gitignore": false,
    ".prettierrc.js": false,
    ".watchmanconfig": false,
    ".cz-config.js": false,
    "package-lock.json": false,
    "CHANGELOG.md": false,
    "babel.config.js": false,
    "node_modules": false,
    "ios": false,
    "android": false,
    ".vscode": false,
    "doc": false,
    "plugins/century-comic/.editorconfig": false,
    "plugins/century-comic/.eslintignore": false,
    "plugins/century-comic/.eslintrc.js": false,
    "plugins/century-comic/.gitignore": false,
    "plugins/century-comic/esbuild.config.js": false,
    "plugins/century-comic/LICENSE": false,
    "plugins/century-comic/package-lock.json": false,
    "plugins/century-comic/node_modules": false,
    "plugins/century-comic/dist": false,
    "plugins/century-comic/.vscode": false,
    "packages/nodejs-nawb": false,
    "packages/nawb-template": false,
    "packages/nawb-bridge/.eslintignore": false,
    "packages/nawb-bridge/.eslintrc.js": false,
    "packages/nawb-bridge/.gitignore": false,
    "packages/nawb-bridge/esbuild.config.js": false,
    "packages/nawb-bridge/LICENSE": false,
    "packages/nawb-bridge/package-lock.json": false,
    "packages/nawb-bridge/node_modules": false,
    "packages/nawb-bridge/.editorconfig": false,
    "packages/nawb-bridge/dist": false,
    "packages/nawb-bridge/.vscode": false,
    "packages/nawb/.vscode": false,
    "packages/nawb/.vscode/settings.json": false,
    "packages/nawb/dist": false,
    "packages/nawb/doc": false,
    "packages/nawb/node_modules": false,
    "packages/nawb/scripts": false,
    "packages/nawb/test": false,
    "packages/nawb/test/index.js": false,
    "packages/nawb/.editorconfig": false,
    "packages/nawb/.eslintignore": false,
    "packages/nawb/.eslintrc.js": false,
    "packages/nawb/.gitignore": false,
    "packages/nawb/CHANGELOG.md": false,
    "packages/nawb/esbuild.config.js": false,
    "packages/nawb/LICENSE": false,
    "packages/nawb/package-lock.json": false,
    ".git": false
  },
  "workspace.isHidden": false
}