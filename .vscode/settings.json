{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignoreWords": [
    "carla",
    "miao",
    "mutex"
  ],
  "files.associations": {
    ".js": "javascript",
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.gyp": "python",
    "mutex": "cpp"
  },
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "files.exclude": {
    ".buckconfig": true,
    ".editorconfig": true,
    ".eslintignore": true,
    ".eslintrc.js": true,
    ".gitattributes": true,
    ".gitignore": true,
    ".prettierrc.js": true,
    ".watchmanconfig": true,
    ".cz-config.js": true,
    "package-lock.json": true,
    "CHANGELOG.md": true,
    "babel.config.js": true,
    "node_modules": true,
    "ios": true,
    "android": true,
    ".vscode": true,
    "doc": true,
    "plugins/century-comic/.editorconfig": true,
    "plugins/century-comic/.eslintignore": true,
    "plugins/century-comic/.eslintrc.js": true,
    "plugins/century-comic/.gitignore": true,
    "plugins/century-comic/esbuild.config.js": true,
    "plugins/century-comic/LICENSE": true,
    "plugins/century-comic/package-lock.json": true,
    "plugins/century-comic/node_modules": true,
    "plugins/century-comic/dist": true,
    "plugins/century-comic/.vscode": true,
    "packages/nodejs-carla": true,
    "packages/carla-template": true,
    "packages/carla-bridge/.eslintignore": true,
    "packages/carla-bridge/.eslintrc.js": true,
    "packages/carla-bridge/.gitignore": true,
    "packages/carla-bridge/esbuild.config.js": true,
    "packages/carla-bridge/LICENSE": true,
    "packages/carla-bridge/package-lock.json": true,
    "packages/carla-bridge/node_modules": true,
    "packages/carla-bridge/.editorconfig": true,
    "packages/carla-bridge/dist": true,
    "packages/carla-bridge/.vscode": true,
    "packages/carla/.vscode": true,
    "packages/carla/.vscode/settings.json": true,
    "packages/carla/dist": true,
    "packages/carla/doc": true,
    "packages/carla/node_modules": true,
    "packages/carla/scripts": true,
    "packages/carla/test": true,
    "packages/carla/test/index.js": true,
    "packages/carla/.editorconfig": true,
    "packages/carla/.eslintignore": true,
    "packages/carla/.eslintrc.js": true,
    "packages/carla/.gitignore": true,
    "packages/carla/CHANGELOG.md": true,
    "packages/carla/esbuild.config.js": true,
    "packages/carla/LICENSE": true,
    "packages/carla/package-lock.json": true,
    ".git": true
  },
  "workspace.isHidden": true
}