{
  "name": "com.deskbtm.nawb",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "windows": "react-native run-windows",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "release": "standard-version",
    "bundle:android": "react-native bundle --platform android --minify true  --entry-file index.js --bundle-output ./dist/android/index.android.bundle --assets-dest ./dist/android/assets",
    "tree": "node ./doc/tree"
  },
  "dependencies": {
    "@react-native/assets": "file:third_party/react-native/packages/assets",
    "@react-native/normalize-color": "file:third_party/react-native/packages/normalize-color",
    "@react-native/polyfills": "file:third_party/react-native/packages/polyfills",
    "axios": "^0.24.0",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "react": "17.0.2",
    "react-native": "link:third_party/react-native",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/cli": "^6.0.0",
    "@react-native-community/cli-platform-android": "^6.0.0",
    "@react-native-community/cli-platform-ios": "^6.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^25.2.3",
    "@types/metro-config": "^0.66.0",
    "@types/react": "17.0.2",
    "@types/react-native": "^0.66.3",
    "@types/react-native-background-downloader": "^2.3.3",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "fs-extra": "^10.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "standard-version": "^9.1.1",
    "tree-node-cli": "^1.4.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "metro": "> 0.60.0"
  },
  "workspaces": {
    "packages": [
      "libs/*"
    ]
  },
  "engines": {
    "node": ">= 14.16.0",
    "npm": "Please use yarn-3.1.1",
    "yarn": ">2.0.0 <4.0.0",
    "pnpm": "Please use yarn-3.1.1"
  },
  "jest": {
    "preset": "react-native",
    "roots": [
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/packages",
      "<rootDir>/android"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](packages)[/\\\\]",
      "<rootDir>[/\\\\](android)[/\\\\]"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
